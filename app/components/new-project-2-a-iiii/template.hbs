<div class="modal-extra-slider half-size">
  <h1 class="modal-extra-slider-header">Add Resources - Required Resources</h1>

  {{#each this.processPropertiesNew as | processProperty |}}
    <p>{{processProperty.name}}</p>
    {{input value=processProperty.property_value}}
  {{/each}}


  <footer>
    <button {{action "previous"}}>Previous</button>
    <button {{action "sendProcessProperties"}}>Add Resource</button>
  </footer>
</div>


