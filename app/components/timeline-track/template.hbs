<p class='track-name'>{{this.project.name}}</p>
{{#each this.project.phases as |phase|}}
  {{
    timeline-object
    name=phase.name
    cost=phase.cost
    start=phase.start_date
    end=phase.end_date
    timelineGridObjects=this.timelineGridObjects
    onInteractionEnd=this.onInteractionEnd
  }}
{{/each}}
